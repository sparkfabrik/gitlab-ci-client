{
    "name": "sparkfabrik/gitlab-ci-client",
    "description": "Docker image and script that wraps the gitlab rest API.",
    "type": "project",
    "require": {
        "m4tthumphrey/php-gitlab-api": "^11.6",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/console": "^5.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sparkfabrik\\GitlabCiClient\\": "src/"
        }
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "bin-dir": "bin/",
        "preferred-install": "dist"
    },
    "authors": [
        {
            "name": "Marco Primitivo",
            "email": "marco.primitivo@sparkfabrik.com"
        }
    ],
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "zenstruck/console-test": "^1.3"
    }
}
